# Example `.env.testing` for running tests locally.
# Copy this file to `.env.testing` and fill with your test DB credentials.
# IMPORTANT: Use a dedicated **test** database (do NOT point this at your production DB).

APP_NAME=Laravel
APP_ENV=testing
APP_KEY=base64:GENERATE_YOUR_OWN
APP_DEBUG=true
APP_URL=http://localhost

DB_CONNECTION=pgsql
DB_HOST=your-supabase-host.supabase.co
DB_PORT=5432
DB_DATABASE=your_test_database
DB_USERNAME=your_db_user
DB_PASSWORD=your_db_password

# If you prefer to use sqlite in-memory for tests, set:
# DB_CONNECTION=sqlite
# DB_DATABASE=:memory:
